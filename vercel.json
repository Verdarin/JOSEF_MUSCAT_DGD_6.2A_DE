{
    "version": 2,
    "builds": [
        {
            "src": "project/main.py",
            "use": "@vercel/python",
            "config": {
                "command": "uvicorn project.routes.main:app --host 0.0.0.0 --port 8000"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "project/main.py"
        }
    ]
}